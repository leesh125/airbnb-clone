container_commands:
  01_migrate:
    command: "source /var/app/venv/*/bin/activate ; python manage.py migrate"
    leader_only: true
  02_seed_amenities:
    command: "source /var/app/venv/*/bin/activate ; python manage.py seed_amenities"
    leader_only: true
  03_seed_facilities:
    command: "source /var/app/venv/*/bin/activate ; python manage.py seed_facilities"
    leader_only: true
  04_seed_users:
    command: "source /var/app/venv/*/bin/activate ; python manage.py seed_users --number 50"
    leader_only: true
  05_seed_rooms:
    command: "source /var/app/venv/*/bin/activate ; python manage.py seed_rooms --number 150"
    leader_only: true
  06_seed_reviews:
    command: "source /var/app/venv/*/bin/activate ; python manage.py seed_reviews --number 200"
    leader_only: true
  07_seed_lists:
    command: "source /var/app/venv/*/bin/activate ; python manage.py seed_lists --number 50"
    leader_only: true
  08_seed_reservations:
    command: "source /var/app/venv/*/bin/activate ; python manage.py seed_reservations --number 100"
    leader_only: true
option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: config.wsgi:application
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: config.settings
  aws:elasticbeanstalk:environment:proxy:staticfiles:
    /static: static